<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapletan.demo.database.AccountMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mapletan.demo.database.dataobject.AccountDO">
        <id column="account_id" property="accountId" />
        <result column="total_capital" property="totalCapital" />
        <result column="available_capital" property="availableCapital" />
        <result column="frozen_capital" property="frozenCapital" />
        <result column="settle_capital" property="settleCapital" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        account_id, total_capital, available_capital, frozen_capital, settle_capital, create_time, update_time
    </sql>

</mapper>
