<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapletan.demo.database.InventoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mapletan.demo.database.dataobject.InventoryDO">
        <id column="id" property="id" />
        <result column="portfolio_id" property="portfolioId" />
        <result column="sec_id" property="secId" />
        <result column="holding_quantity" property="holdingQuantity" />
        <result column="frozen_holding_quantity" property="frozenHoldingQuantity" />
        <result column="cost_price" property="costPrice" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, portfolio_id, sec_id, holding_quantity, frozen_holding_quantity, cost_price, create_time, update_time
    </sql>

</mapper>
